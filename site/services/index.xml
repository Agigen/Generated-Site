                        &lt;/g&gt;

                        &lt;g class=&#34;proddev-process__point proddev-process__point--4&#34;&gt;
                            &lt;circle fill=&#34;#FFFFFF&#34; stroke=&#34;#FF0066&#34; stroke-width=&#34;3&#34; stroke-linecap=&#34;round&#34; stroke-linejoin=&#34;round&#34; stroke-miterlimit=&#34;10&#34; cx=&#34;515.9&#34; cy=&#34;704.2&#34; r=&#34;26.8&#34;/&gt;
